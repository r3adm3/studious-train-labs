   $c = $fibarray[2]
   $total = 0

<# print variables test 
   $fibarray[0]
   $fibarray[1]
   $fibarray[2] #>

   $count = $fibarray.Count
 <# do for loop #>
     <# assign value to $i #>
     for($i=0;

     <# iterate through loop till i is less count #>
     $i -le $count;

     <# adds 1 to $i #>
     $i++)

     <# fib logic #>
     {
     <# test to print Write-Host $c #>
